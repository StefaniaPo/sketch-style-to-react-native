{
  "name": "sketch-style-to-react-native",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketch-style-to-react-native",
    "manifest": "src/manifest.json",
    "main": "style-to-react-native.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:futantan/sketch-style-to-react-native.git"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Tantan Fu <luckytantanfu@gmail.com>",
  "dependencies": {
    "css-to-react-native": "^2.2.0"
  }
}